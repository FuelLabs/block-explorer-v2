{
  "name": "explorer",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "npm": ">=8.0.0"
  },
  "homepage": "./",
  "dependencies": {
    "@apollo/client": "^3.4.13",
    "@craco/craco": "^6.4.3",
    "@ethersproject/bignumber": "^5.5.0",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.20.37",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.15",
    "bignumber.js": "^9.0.1",
    "fuels": "^0.0.0-master-9a71ed8",
    "graphql": "^15.7.2",
    "qrcode.react": "^1.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "styled-components": "^5.3.3",
    "typescript": "^4.4.4",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "postbuild": "yarn create404",
    "create404": "cp ./build/index.html ./build/404.html",
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --check src",
    "format:fix": "prettier --write src",
    "cypress:open": "cypress open",
    "build-operations": "graphql-codegen && prettier --write src/api/__generated__/types.ts",
    "update-schema": "get-graphql-schema http://localhost:4000/graphql > fuel-core-schema.graphql",
    "postinstall": "npm run build-operations"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "eol-last": [
        "error",
        "always"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/near-operation-file-preset": "^2.2.6",
    "@graphql-codegen/typescript": "^2.4.5",
    "@graphql-codegen/typescript-operations": "^2.3.2",
    "@graphql-codegen/typescript-react-apollo": "^3.2.8",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "@types/qrcode.react": "^1.0.2",
    "@types/react": "^17.0.34",
    "@types/react-router-dom": "^5.3.2",
    "bundle-stats-webpack-plugin": "^3.3.2",
    "cypress": "^8.7.0",
    "eslint-plugin-cypress": "^2.12.1",
    "get-graphql-schema": "^2.1.2",
    "graphqurl": "^1.0.1",
    "prettier": "2.5.1",
    "semantic-release": "^18.0.0",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}
